{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "prepare": {},
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:mock": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "mock": {
      "cache": false
    }
  }
}
